<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/mainpage2"
    tools:context=".MainActivity">


    <com.google.android.material.button.MaterialButton
        android:id="@+id/landlordButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Landlord Screen"
        app:layout_constraintBottom_toTopOf="@id/studentScreenButton"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.749"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.084" />

    <com.google.android.material.button.MaterialButton
        android:id="@+id/studentScreenButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Student Screen"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="64dp"
        tools:layout_editor_absoluteY="583dp" />


</androidx.constraintlayout.widget.ConstraintLayout>